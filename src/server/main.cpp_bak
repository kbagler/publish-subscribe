#include <iostream>

#include <asio.hpp>
#include "ServerTCP.h"

int main(int argc, char *argv[])
{
	try
	{
		asio::io_context io_context;
		tcp_server server(io_context, 9999);
		io_context.run();
	}
	catch (std::exception& e)
	{
		std::cerr << e.what() << std::endl;
	}

	return 0;
}

